{% macro render(common, workspaceId, workspaces) %}
    <div class="swiper room-swiper">
        <div class="swiper-wrapper">
        {%- for room in workspaces[workspaceId].rooms %}

            <div class="ws-room-slide swiper-slide{% if loop.first %} selected {% endif %}">
            <a href="#/{{ room.slug }}">{{ room.name }}</a>
            </div>
        {%- endfor %}
        </div>
    </div>

{% endmacro %}
