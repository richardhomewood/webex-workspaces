{
  "license": "MIT",
  "name": "eleventy-webpack",
  "repository": {
    "type": "git",
    "url": "https://github.com/clenemt/eleventy-webpack"
  },
  "engines": {
    "node": "14"
  },
  "scripts": {
    "clean": "rimraf _site webex-workspaces-site.zip",
    "format": "prettier --write **/*.{js,s?css,yml} !_site",
    "start": "cross-env NODE_ENV=development npm-run-all -l clean --parallel start:*",
    "start:webpack": "webpack --watch",
    "start:eleventy": "eleventy --serve",
    "build": "cross-env NODE_ENV=production npm-run-all -l clean build:*",
    "build:webpack": "webpack",
    "build:eleventy": "eleventy",
    "package": "cross-env NODE_ENV=production npm-run-all -l clean build:* package:zip",
    "package:zip": "(cd _site; zip -9 -r ../webex-workspaces-site.zip content/ workspaces/)",
    "analyze": "npm-run-all build analyze:*",
    "analyze:js": "source-map-explorer _site/**/*.js _site/**/*.js.map"
  },
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-img": "1.0.0",
    "@11ty/eleventy-navigation": "0.3.2",
    "@babel/cli": "7.17.0",
    "@babel/core": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "babel-loader": "8.2.3",
    "cheerio": "1.0.0-rc.10",
    "core-js": "3.21.0",
    "cross-env": "7.0.3",
    "css-loader": "6.6.0",
    "css-minimizer-webpack-plugin": "3.4.1",
    "date-fns": "2.28.0",
    "html-minifier": "4.0.0",
    "js-yaml": "4.1.0",
    "markdown-it": "12.3.2",
    "markdown-it-anchor": "8.4.1",
    "markdown-it-attrs": "4.1.3",
    "markdown-it-bracketed-spans": "1.0.1",
    "mini-css-extract-plugin": "2.5.3",
    "npm-run-all": "4.1.5",
    "outdent": "0.8.0",
    "postcss": "8.4.6",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.3.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sass": "1.49.7",
    "sass-loader": "12.4.0",
    "source-map-explorer": "2.5.2",
    "svg-sprite": "1.5.4",
    "terser-webpack-plugin": "5.3.1",
    "vinyl": "2.2.1",
    "webpack": "5.68.0",
    "webpack-cli": "4.9.2",
    "webpack-manifest-plugin": "4.1.1"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "swiper": "^8.0.6",
    "terser-webpack-plugin": "^5.3.1"
  }
}
